{
    "name": "phpactor/container",
    "type": "library",
    "description": "Phpactor's DI Container",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "phpactor/map-resolver": "dev-allow-enums",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpstan/phpstan": "~0.12.0",
        "phpunit/phpunit": "^9.0",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/przepompownia/map-resolver"
        }
    ],
    "autoload": {
        "psr-4": {
            "Phpactor\\Container\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Container\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "integrate": [
            "vendor/bin/php-cs-fixer fix",
            "vendor/bin/phpstan analyse lib -c phpstan.neon",
            "vendor/bin/phpunit"
        ]
    }
}
